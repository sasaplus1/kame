{
  "extends": "@tsconfig/strictest",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
    "lib": ["DOM"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "sourceMap": true,
    "target": "ESNext"
  }
}
